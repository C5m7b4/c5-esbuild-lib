{
  "name": "c5-esbuild-lib",
  "version": "0.0.1",
  "description": "test library for esbuild",
  "main": "./dist/index/js",
  "module": "./dist/index.esm.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && node build.js",
    "build:long": "esbuild src/index.ts --bundle --outdir=dist --sourcemap --target=es2015 --format=esm --define:process.env.NODE_ENV=\"development\"",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "view:coverage": "start coverage/lcov-report/index.html",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.32.1",
    "@swc/core": "^1.3.44",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "12.1.5",
    "@types/jest": "^29.5.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "esbuild": "^0.17.14",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.37.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "npm-dts": "^1.3.12",
    "prettier": "^2.8.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  }
}
